{
  "name": "ts-notifications",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@trading/energies-ui": "bitbucket:empica/ts-ui-build#b331910",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "classnames": "^2.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-use-websocket": "^4.2.0",
    "sass": "^1.54.9",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run start-ws && craco start",
    "start:local": "dotenv -e .env.local -- craco start",
    "start:development": "dotenv -e .env.development --host 0.0.0.0 craco start",
    "start:staging": "dotenv -e .env.staging -- craco start",
    "build": "craco build",
    "build:development": "dotenv -e .env.development -- craco build && npm run order-css",
    "build:staging": "dotenv -e .env.staging -- craco build && npm run order-css",
    "build:production": "dotenv -e .env.production -- craco build && npm run order-css",
    "test": "craco test",
    "eject": "craco eject",
    "update-energies-ui": "npm i @trading/energies-ui",
    "start-ws": "node ws-server.js &",
    "order-css": "node build-order-css.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "16.16.0"
  },
  "devDependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@craco/craco": "^7.1.0",
    "@uiw/react-codemirror": "^4.21.9",
    "dayjs": "^1.11.9",
    "dotenv-cli": "^7.1.0",
    "jsdom": "^17.0.0",
    "mini-css-extract-plugin": "^2.7.6"
  }
}